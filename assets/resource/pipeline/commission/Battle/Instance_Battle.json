            "param": {
                "template": [
                    "战斗/角色/持续技能蓝耗0.png"
                ],
                // "roi": [
                //     850,
                //     560,
                //     100,
                //     120
                // ]
                "roi": "释放状态Q",
                "roi_offset": [
                    -17,
                    -45,
                    50,
                    75
                ]
            }
        },
        "action": {
            "type": "Click",
            "param": {
                //"target": true
                "target": "释放状态Q"
            }
