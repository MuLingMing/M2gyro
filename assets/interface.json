{
    // 本文件主要定义了通用 UI 的行为和显示内容
    // 可参考文档和示例添加更多您需要的功能
    // 文档：https://maafw.xyz/docs/3.3-ProjectInterfaceV2
    // 示例：https://github.com/MaaXYZ/MaaFramework/blob/main/sample/interface.json
    "interface_version": 2,
    "name": "M2gyro",
    "description": "这是一个使用 MaaFramework 创建的新项目,目标为二重螺旋。",
    "contact": "QQ群: 暂无",
    "license": "MIT",
    "welcome": "首次使用！欢迎使用 M2gyro!",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/MuLingMing/M2gyro",
    "version": "0.1.1-alpha",
    // Mirror酱为第三方分发服务，新项目默认关闭
    // 若有需要请联系 Mirror酱 开通，直接自行开启是无法使用的
    // "mirrorchyan_rid": "MaaXXX",
    // "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720 // 默认缩放分辨率的短边长度
        } /*,
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio",
                // 截图及控制方式，请根据项目实际情况修改
                // 文档: https://maafw.xyz/docs/2.4-ControlMethods
                "screencap": "PrintWindow", // 截图方式
                "mouse": "PostMessageWithCursorPos", // 鼠标控制方式
                "keyboard": "PostMessageWithCursorPos" // 键盘控制方式
            }
        }*/
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "./resource"
            ]
        },
        {
            "name": "PC",
            "path": [
                "./resource",
                "./resource_win32"
            ]
        }
    ],
    // 可选的 agent 示例
    // "agent": {
    //     "child_exec": "python",
    //     "child_args": [
    //         "./agent/main.py"
    //     ]
    // },
    "task": [
        {
            "name": "驱离",
            "entry": "驱离",
            "description": "测试版，仅限从驱离进入，无主控技能，仅复位",
            "option": [
                "重复次数",
                "超时时间",
                "倍率选择"
            ]
        },
        {
            "name": "选项任务",
            "entry": "MyTask2",
            "option": [
                "任务选项1"
            ]
        },
        {
            "name": "参数任务",
            "entry": "MyTask3",
            "option": [
                "任务选项1"
            ],
            "pipeline_override": {}
        },
        {
            "name": "带Custom的任务",
            "entry": "MyTask4"
        }
    ],
    "option": {
        "重复次数": {
            "type": "select",
            "cases": [
                {
                    "name": "10000次"
                },
                {
                    "name": "1次",
                    "pipeline_override": {
                        "驱离": {
                            "max_hit": 1
                        }
                    }
                },
                {
                    "name": "15次",
                    "pipeline_override": {
                        "驱离": {
                            "max_hit": 15
                        }
                    }
                },
                {
                    "name": "50次",
                    "pipeline_override": {
                        "驱离": {
                            "max_hit": 50
                        }
                    }
                }
            ]
        },
        "超时时间": {
            "type": "select",
            "cases": [
                {
                    "name": "60秒",
                    "pipeline_override": {
                        "驱离地图": {
                            "timeout": 60000
                        }
                    }
                },
                {
                    "name": "90秒",
                    "pipeline_override": {
                        "驱离地图": {
                            "timeout": 90000
                        }
                    }
                }
            ]
        },
        "倍率选择": {
            "type": "select",
            "cases": [
                {
                    "name": "无加成"
                },
                {
                    "name": "1倍绿书",
                    "pipeline_override": {
                        "倍率选择": {
                            "recognition": {
                                "param": {
                                    "template": [
                                        "委托手册/1绿书_图标.png"
                                    ],
                                    "roi": [
                                        165,
                                        110,
                                        950,
                                        480
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "2倍蓝书",
                    "pipeline_override": {
                        "倍率选择": {
                            "recognition": {
                                "param": {
                                    "template": [
                                        "委托手册/2蓝书_图标.png"
                                    ],
                                    "roi": [
                                        165,
                                        110,
                                        950,
                                        480
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "8倍蓝书",
                    "pipeline_override": {
                        "倍率选择": {
                            "recognition": {
                                "param": {
                                    "template": [
                                        "委托手册/8紫书_图标.png"
                                    ],
                                    "roi": [
                                        165,
                                        110,
                                        950,
                                        480
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "20倍金书",
                    "pipeline_override": {
                        "倍率选择": {
                            "recognition": {
                                "param": {
                                    "template": [
                                        "委托手册/20金书_图标.png"
                                    ],
                                    "roi": [
                                        165,
                                        110,
                                        950,
                                        480
                                    ]
                                }
                            }
                        }
                    }
                }
            ]
        },
        "任务选项1": {
            "type": "select",
            "cases": [
                {
                    "name": "文本1",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本1"
                        }
                    }
                },
                {
                    "name": "文本2",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本2"
                        }
                    }
                }
            ]
        }
    }
}